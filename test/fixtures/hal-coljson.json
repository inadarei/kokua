        {"full-name": "M. Smith", "h:label": "Full Name"},
        {"email": "msmith@example.org", "h:label": "E-Mail"}
      ],
      "h:link": [
        {
          "rel": ["blog"],
          "uri": "http://examples.org/blogs/msmith",
          "h:label": "Blog"
